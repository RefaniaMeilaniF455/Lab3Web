# Membuat Form
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Form</title>
    <style>
       form p  > label {
        display: inline-block;
        width: 100px;
       } 
       form input[type="text"], form textarea {
        border: 1px solid #f315bb;
       }
       from input[type="submit"] {
        border: 1px solid #f315bb;
        background-color: #f315bb;
        color: #e4f074;
        font-weight: bold;
        padding: 5px 15px;
       }
    </style>
</head>
<body>
    <header>
        <h1>Membuat Form</h1>
    </header>
    <form action="proses.php" method="post">
        <fieldset>
            <legend>Data Pelanggan</legend>
            <p>
                <label for="nama">Nama</label>
                <input type="text" id="nama" name="nama">
            </p>
            <p>
                <label for="alamat">Alamat</label>
                <textarea id="alamat" name="alamat" cols="20" rows="3"></textarea>
            </p>
            <p>
                <label>Jenis Kelamin</label>
                <input id="jk_1" type="radio" name="kelamin" value="L" /><label for="jk_l">Laki-laki</label>
                <input id="jk_p" type="radio" name="kelamin" value="P" /><label for="jk_p">Perempuan</label>
            </p>
            <p><input type="submit" value="Login"</p>
        </fieldset>
    </form>
</body>
</html>

# Menampilkan dropdown menu dan listbox dengan multiple selection.
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Membuat Table dengan Dropdown dan Listbox</title>
</head>
<body>
    <header>
        <h1>Membuat Table</h1>
    </header>
    
    <!-- Table -->
    <table border="1" cellpadding="4" cellspacing="0">
        <thead>
            <tr>
                <th>No.</th>
                <th>Fakultas</th>
                <th>Program Studi</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1.</td>
                <td>Teknik</td>
                <td>Teknik Informasi</td>
            </tr>
            <tr>
                <td>2.</td>
                <td>Teknik</td>
                <td>Teknik Industri</td>
            </tr>
            <tr>
                <td>3.</td>
                <td>Teknik</td>
                <td>Teknik Lingkungan</td>
            </tr>
        </tbody>
    </table>
    
    <br>
    
    <!-- Dropdown menu -->
    <label for="fakultas">Pilih Fakultas:</label>
    <select id="fakultas">
        <option value="Teknik">Teknik</option>
        <option value="Ekonomi">Ekonomi</option>
        <option value="Hukum">Hukum</option>
    </select>

    <br><br>

    <!-- Listbox with multiple selection -->
    <label for="prodi">Pilih Program Studi (Bisa pilih lebih dari satu):</label>
    <select id="prodi" multiple size="5">
        <option value="Teknik Informasi">Teknik Informasi</option>
        <option value="Teknik Industri">Teknik Industri</option>
        <option value="Teknik Lingkungan">Teknik Lingkungan</option>
        <option value="Manajemen">Manajemen</option>
        <option value="Akuntansi">Akuntansi</option>
    </select>

    <br><br>

    <!-- Button to submit -->
    <button type="submit">Submit</button>
    
</body>
</html>
